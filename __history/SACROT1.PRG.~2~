                ELSEIF ! EMPTY(ped_s->pexp_dat) .AND. ((mdata_sis - ped_s->pexp_dat) > m_set[1,121]) .AND. ! EMPTY(m_set[1,121])
                        SELE(msele);IF(morde>0,ORDSETFOCUS(morde),)
