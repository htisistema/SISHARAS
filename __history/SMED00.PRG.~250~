// CONSULTA

        ELSEIF MenuId=MENUCONS_1  
                f7_cli()
        */
        ELSEIF MenuId=MENUCONS_2  
*                                       f7_animal()
                                        smed310()       && CONSULTA CLIENTES
        /*
