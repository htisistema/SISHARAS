****************
* MENU PRINCIPAL
****************
FUNCTION smed0()
****************
MEMVAR mdata_sis,memp_resa,mversao,cod_operado,mcaminho,mol,sistema
LOCAL opcao,tela,malinhar:={},aret:={},i:=0

PUBLIC setah,setav,setcim := CHR(24),setbaix := CHR(25),setesq := CHR(27),setdir := CHR(26)
       //rl_pd[15]

PRIVATE mcod_vend,mmens
SETCOLOR('W/B,B/w,,,GR/B')
mn_sac0 := op_tela(00,00,45,120,memp_resa+' - CNPJ: '+mcgc_firm+SPACE(20)+'.: SISHARAS '+mversao+' :.'+SPACE(20)+'Data: '+DTOC(mdata_sis)+' - '+ver_dia(mdata_sis)+'   Serie: '+ver_serie(),1,,,)
//Wvw_SetTitle( ,'.:'+memp_resa+':.'+SPACE(40)+'.: SISHARAS '+mversao+' :.'+SPACE(40)+'Data:'+DTOC(mdata_sis))
//wvw_noclose()
wvw_maximize(0)
WVW_CenterWindow(,.T.,.T.)
SET CURSOR OFF
botao(00,00,50,120)
WVW_DrawImage( ,00,00,45,120,ALLTRIM(m_indiv[1,3])+'HTILOGO.JPG',.F.,.T.)
WVW_DrawImage( ,35,55,41,115,ALLTRIM(m_indiv[1,3])+"rodape_pinpad.JPG",.F.,.F.)
IF FILE(ALLTRIM(m_indiv[1,3])+'HTIFIRMA.JPG')
        WVW_DrawImage( ,05,10,32,80,ALLTRIM(m_indiv[1,3])+'HTIFIRMA.JPG',.T.,.F.)
ELSEIF FILE(ALLTRIM(m_indiv[1,3])+'HTIFIRMA1.JPG')
        WVW_DrawImage( ,09,04,34,90,ALLTRIM(m_indiv[1,3])+'HTIFIRMA1.JPG',.T.,.F.)
ENDIF
botao(06,97,18,MAXCOL()-5)
IF FILE(ALLTRIM(m_indiv[1,3])+cod_operado+'.jpg')
        WVW_DrawImage( ,06,97,18,MAXCOL()-5,ALLTRIM(m_indiv[1,3])+cod_operado+'.jpg',.T.,.F.)
ELSE
        WVW_DrawImage( ,06,97,18,MAXCOL()-5,ALLTRIM(m_indiv[1,3])+'HTIusu.jpg',.T.,.F.)
ENDIF
WVW_DrawLabel(,20,MAXCOL()-19,'Operador: '+cod_operado,,,RGB(25,25,112),RGB(255,250,250), 'times',18,8,,,,,)

WVW_DrawLabel(,40,5,' '+mversao+' ',,,RGB(25,25,112),RGB(255,250,250), 'times',30,15,,,,,)

set key 23 to
opcao:=0
SET KEY 28 TO menu_cons()       // F1 MENU DE CONSULTAS

SETCOLOR('W/B+,B/w,,,GR+/B')
SET KEY 287 TO alt_senha        // ALT+S
mtela_principal := WVW_SAVESCREEN(,00,00,MAXROW(),MAXCOL())
exibi_prg('SMED0')

//WVW_TBCreate( , .T.,TBSTYLE_LIST            , 0,3,3)
WVW_TBCreate( , .T.,TBSTYLE_FLAT            , 0,55,50)
WvW_PBSetFont( NIL, "ARIAL BLACK", 16, 0,)

WVW_TBAddButton( ,MENUCONS_1,ALLTRIM(m_indiv[1,3])+'cliente.bmp','<C>lientes',IF(FILE(ALLTRIM(m_indiv[1,3])+'cliente.bmp'),0,1))
WVW_TBAddButton( ,MENUCONS_2,ALLTRIM(m_indiv[1,3])+'animais.bmp','<C>ons.Animais',IF(FILE(ALLTRIM(m_indiv[1,3])+'animais.bmp'),0,1),.F.)
WVW_TBAddButton( ,MENUPED_1,ALLTRIM(m_indiv[1,3])+'pedido.bmp','<V> E N D A',IF(FILE(ALLTRIM(m_indiv[1,3])+'pedido.bmp'),0,1),.F.)
WVW_TBAddButton( ,MENUCX_1,ALLTRIM(m_indiv[1,3])+'recebimento.bmp','<R>ecebimento',IF(FILE(ALLTRIM(m_indiv[1,3])+'recebimento.bmp'),0,1),.F.)
WVW_TBAddButton( ,MENUDUPR_15,ALLTRIM(m_indiv[1,3])+'baixar.bmp','<B>aixa Doc.',IF(FILE(ALLTRIM(m_indiv[1,3])+'baixar.bmp'),0,1))
WVW_TBAddButton( ,MENUNF_9,ALLTRIM(m_indiv[1,3])+'nfe.bmp','<CTRL+E>NF-e',IF(FILE(ALLTRIM(m_indiv[1,3])+'nfe.bmp'),0,1))
WVW_TBAddButton( ,NBotao9,ALLTRIM(m_indiv[1,3])+'sair.bmp','<ESC> Sair',IF(FILE(ALLTRIM(m_indiv[1,3])+'sair.bmp'),0,1),.F.)


WVW_SBCreate()
WVW_SBAddPart(,,550,0,.F.)
WVW_SBSetText(,0,'{SMED00}')

WVW_SBAddPart(,,350,0,.F.)
WVW_SBSetText(,1,'<< Operador: '+ALLTRIM(cod_operado)+' >>  << Terminal: '+NETNAME()+' >>  << IP Terminal: '+GetIp()+' >>')

WVW_SBAddPart(,,150,0,.F.)
WVW_SBSetText(,2,'<< IP Servidor: '+ALLTRIM(m_cfg[4])+' >>')

WVW_SBAddPart(,,150,0,.F.)
IF ! EMPTY(m_set[1,82])
	IF cod_operado = '999'
        	WVW_SBSetText(,3,'   ** FIREBIRD ** '+STRZERO(DATE() - CTOD(SUBSTR(m_set[1,82],1,2)+'/'+SUBSTR(m_set[1,82],3,2)+'/'+SUBSTR(m_set[1,82],5,2)),5))
	ELSE
        	WVW_SBSetText(,3,'   ** Banco: FIREBIRD **')
        ENDIF
ELSE
        WVW_SBSetText(,3,'   >> Banco: FIREBIRD <<')
ENDIF
senha_lib()
