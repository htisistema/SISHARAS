*********************
* TELA MARLIZETE
*********************

FUNCTION smed(mmed)
******************

LOCAL opcao

PRIVATE dsemana,vcor[8]

PUBLIC senha_acess,nivel_acess,cod_operado,mmedico,cab_med,cor

PUBLIC memp,resa,crm,cpf,fir,ma,mversao,mesp,mold,mdata_sis,mrelatorio,impressora

******* T I P O   D E   S I S T E M A **************
sistema := 'Controle Medico'
mversao := 'Ver.: 6.0'
****************************************************
cor := SETCOLOR()
fir := 'INS'
ma := 'ERCOM-Informatica Ltda.'
mdata_sis := DATE()
************************************************
*mold := 'ÖÄ·º½ÄÓº'      // Molduras das janelas
mold := 'ÚÄ¿³ÙÄÀ³'      // Molduras das janelas
*************************************************

CLEAR

mrelatorio := 'LPT1'
impressora := 'M'
senha_acess := SPACE(8)
nivel_acess := SPACE(1)
cod_operado := SPACE(3)

SET KEY 28 TO f1_help()
SET KEY -1 TO f2_cli()
SET KEY -2 TO f3_conv()
SET KEY -3 TO f4_med()
SET KEY -4 TO agenda()
SET KEY -9 TO ctrl_w()
SET KEY -41 TO f12_cid()
SET KEY 276 TO alt_data()       // alt + t

SET MESSAGE TO 24 CENTER
SET WRAP ON
SET TALK OFF
SET SCOREBOARD OFF
SET INTEN OFF
SET DATE BRITI
SET DATE FORMAT 'dd/mm/yy'
SET DELE ON
SET INTEN ON
SET KEY 287 TO alt_senhaS        // ALT+S
*SET ESCAPE OFF
SET DEFAULT TO
*vcor[1] := 'b+/w,gr+/b+,B,,gr+/w'         && cor PRINCIPAL
*vcor[2] := 'gr+/w,x'                    && cor para MENSAGEM
*vcor[3] := 'gr+/w,x'                    && cor para os SAY
*vcor[4] := 'gr+/b+,x'                      && cor AMARELO
*vcor[5] := 'W+/g+,x'                     && cor FUNDO
*vcor[6] := 'B+/W,x'                       && cor VERMELHA BRILHANTE
*vcor[7] := 'gr+/b+,x'                     && cor LOGOTIPO
*vcor[8] := 'bg+/bg+,bg+/bg+,x'          && cor p/OPCOES

*vcor[1] := 'rb/w,gr+/rb,rb,,gr+/w'         && cor PRINCIPAL
vcor[1] := 'rb/w,gr+/rb,w,,gr+/w'         && cor PRINCIPAL
vcor[2] := 'gr+/w,x'                    && cor para MENSAGEM
vcor[3] := 'gr+/w,x'                    && cor para os SAY
vcor[4] := 'gr+/rb,x'                      && cor AMARELO
*vcor[5] := 'W+/rb,x'                     && cor FUNDO
vcor[5] := 'rb/w,x'                     && cor FUNDO
vcor[6] := 'rb/W,x'                     && cor FUNDO DO I
*vcor[6] := 'N/rb,x'                     && cor FUNDO DO I
vcor[7] := 'w+/b+,x'                    && cor LOGOTIPO
*vcor[8] := 'N/N,N/N,rb,X'              && cor p/SENHAS
vcor[8] := 'N/N,N/N,w,X'              && cor p/SENHAS
/*
vcor[1] := 'R/W,w/B,W,,N/BG'          && cor PRINCIPAL
vcor[2] := 'B/W,x'                   && cor para MENSAGEM
vcor[3] := 'B/W,x'                     && cor para os SAY
vcor[4] := 'n/n,n/n,R,x'                && cor SENHA
vcor[5] := 'b/W,x'                     && cor FUNDO
vcor[6] := 'R/W,x'                     && cor ABRINDO ARQUIVO
vcor[7] := 'w/B+,x'                    && cor LOGOTIPO
vcor[8] := 'bg+/bg+,bg+/bg+,x'          && cor p/OPCOES

vcor[1] := 'RB/W,w/B,W,,N/BG'          && cor PRINCIPAL
vcor[2] := 'B/W,x'                   && cor para MENSAGEM
vcor[3] := 'B/W,x'                     && cor para os SAY
vcor[4] := 'n/n,n/n,R,x'                && cor SENHA
vcor[5] := 'b/W,x'                     && cor FUNDO
vcor[6] := 'RB/W,x'                     && cor ABRINDO ARQUIVO
vcor[7] := 'w/B+,x'                    && cor LOGOTIPO
vcor[8] := 'bg+/bg+,bg+/bg+,x'          && cor p/OPCOES
*/
SETCOLOR(vcor[1])
apaga(00,00,24,79)
WHILE .T.
        ***** DRA.VANIA LIGIA *************
        memp := 'DRA. VAN'
        resa := 'IA LIGIA ARRUDA DE ARAUJO'
        crm := 'CREMEPE: 3532'
        cpf := 'CPF: 080.783.004/68'
        mesp := 'GINECOLOGIA - OBSTETRICIA'
        mmedico := 'VANIA'
        cab_med := 'VANIA'
        *************************************
        MKDIR('\SMED\VANIA')
        SET DEFAULT TO \SMED\VANIA
        CLEAR
        ************** TELA DE APRESENTACAO ************
        tela('"SMED" - Sistema de Controle Medico')
        ************************************************
        SETCOLOR(vcor[1])
        num := 80
        FOR f=1 TO 161-num

                @ 01,00 say SUBSTR(SPACE(num) + PADC('±±±±±Ü  ±±±±±±Ü ±±±±±±Ü     ',80),161-f,num)
                @ 02,00 say SUBSTR(SPACE(num) + PADC('±±Ûß±±Û ±±Ûß±±Û ±±Ûß±±Û     ',80),161-f,num)
                @ 03,00 say SUBSTR(SPACE(num) + PADC('±±Û ±±Û ±±±±±Ûß ±±±±±±Û     ',80),161-f,num)
                @ 04,00 say SUBSTR(SPACE(num) + PADC('±±Û ±±Û ±±Ûß±±Ü ±±Û ±±Û     ',80),161-f,num)
                @ 05,00 say SUBSTR(SPACE(num) + PADC('±±±±±Ûß ±±Û ±±Û ±±Û ±±Û  ±±Ü',80),161-f,num)
                @ 06,00 say SUBSTR(SPACE(num) + PADC(' ßßßßß   ßß  ßß  ßß  ßß   ßß',80),161-f,num)
                @ 07,00 say SUBSTR(SPACE(num) + PADC('±±Ü ±±Ü ±±±±±±Ü ±±±ÜÜ ±±Ü ±±Ü ±±±±±±Ü     ±±Ü     ±±Ü ±±±±±±Ü ±±Ü ±±±±±±Ü ',80),f,num)
                @ 08,00 say SUBSTR(SPACE(num) + PADC('±±Û ±±Û ±±Ûß±±Û ±±Û±±Û±±Û ±±Û ±±Ûß±±Û     ±±Û     ±±Û ±±Ûßßßß ±±Û ±±Ûß±±Û ',80),f,num)
                @ 09,00 say SUBSTR(SPACE(num) + PADC('±±Û ±±Û ±±±±±±Û ±±Û ß±±±Û ±±Û ±±±±±±Û     ±±Û     ±±Û ±±Û±±±Ü ±±Û ±±±±±±Û ',80),f,num)
                @ 10,00 say SUBSTR(SPACE(num) + PADC(' ±±±±Ûß ±±Û ±±Û ±±Û   ±±Û ±±Û ±±Û ±±Û     ±±Û     ±±Û ±±Û ±±Û ±±Û ±±Û ±±Û ',80),f,num)
                @ 11,00 say SUBSTR(SPACE(num) + PADC('  ±±Ûß  ±±Û ±±Û ±±Û   ±±Û ±±Û ±±Û ±±Û     ±±±±±±Ü ±±Û ±±±±±±Û ±±Û ±±Û ±±Û ',80),f,num)
                @ 12,00 say SUBSTR(SPACE(num) + PADC('   ßß    ßß  ßß  ßß    ßß  ßß  ßß  ßß      ßßßßßß  ßß  ßßßßßß  ßß  ßß  ßß ',80),f,num)
        NEXT f
        SETCOLOR(vcor[7])
        men_giro('Insercom Informatica Ltda.',21,02,20)
        SETCOLOR('n/w*,w+/n+,,,w+/n')
        DEVPOS(24,33);DEVOUT('Aguarde, Gerando Arquivos..')
        SETCOLOR(vcor[1])
        dbfs()
ENDDO
*************************** f i m *******************************
******************************
* TELA DA MARLIZETE
******************************

FUNCTION marli
****************

LOCAL opcao

WHILE .T.
        ***** DRA.MARIZETE *************
        memp := 'DRA. MAR'
        resa := 'LIZETE LEOPOLDINA BEZERRA'
        crm := 'CREMEPE: 6285'
        cpf := 'CPF: 016.655.088/41'
        mesp := 'TOCOGINECOLOGIA-PATOLOGIA MAMARIA'
        mmedico := 'MARLI'
        cab_med := 'MARLI'
        *************************************
*       MKDIR('C:\SMED\MARLI')
*       SET DEFAULT TO C:\SMED\MARLI
        MKDIR('\SMED\MARLI')
        SET DEFAULT TO \SMED\MARLI
        CLEAR
        ************** TELA DE APRESENTACAO ************
        tela('"SMED" - Sistema de Controle Medico')
        ************************************************
        SETCOLOR(vcor[1])
        num := 80
        FOR f=1 TO 161-num

                @ 01,00 say SUBSTR(SPACE(num) + PADC('±±±±±Ü  ±±±±±±Ü ±±±±±±Ü     ',80),161-f,num)
                @ 02,00 say SUBSTR(SPACE(num) + PADC('±±Ûß±±Û ±±Ûß±±Û ±±Ûß±±Û     ',80),161-f,num)
                @ 03,00 say SUBSTR(SPACE(num) + PADC('±±Û ±±Û ±±±±±Ûß ±±±±±±Û     ',80),161-f,num)
                @ 04,00 say SUBSTR(SPACE(num) + PADC('±±Û ±±Û ±±Ûß±±Ü ±±Û ±±Û     ',80),161-f,num)
                @ 05,00 say SUBSTR(SPACE(num) + PADC('±±±±±Ûß ±±Û ±±Û ±±Û ±±Û  ±±Ü',80),161-f,num)
                @ 06,00 say SUBSTR(SPACE(num) + PADC(' ßßßßß   ßß  ßß  ßß  ßß   ßß',80),161-f,num)
                @ 07,00 say SUBSTR(SPACE(num) + PADC('±±±ÜÜ±±±Ü ±±±±±±Ü ±±±±±±Ü ±±Ü     ±±Ü ±±±±±±Ü ±±±±±±Ü ±±±±±±Ü ±±±±±±Ü',80),f,num)
                @ 08,00 say SUBSTR(SPACE(num) + PADC('±±Û±±Û±±Û ±±Ûß±±Û ±±Ûß±±Û ±±Û     ±±Û  ßß±±Ûß ±±Ûßßßß  ß±±Ûßß ±±Ûßßßß',80),f,num)
                @ 09,00 say SUBSTR(SPACE(num) + PADC('±±Û ßß±±Û ±±±±±±Û ±±±±±Ûß ±±Û     ±±Û   ±±Û   ±±±±±Ü    ±±Û   ±±±±±Ü ',80),f,num)
                @ 10,00 say SUBSTR(SPACE(num) + PADC('±±Û   ±±Û ±±Û ±±Û ±±Ûß±±Ü ±±Û     ±±Û  ±±Û    ±±Ûßßß    ±±Û   ±±Ûßßß ',80),f,num)
                @ 11,00 say SUBSTR(SPACE(num) + PADC('±±Û   ±±Û ±±Û ±±Û ±±Û ±±Û ±±±±±±Ü ±±Û ±±±±±±Ü ±±±±±±Ü   ±±Û   ±±±±±±Ü',80),f,num)
                @ 12,00 say SUBSTR(SPACE(num) + PADC(' ßß    ßß  ßß  ßß  ßß  ßß  ßßßßßß  ßß  ßßßßßß  ßßßßßß    ßß    ßßßßßß',80),f,num)
        NEXT f
        SETCOLOR(vcor[7])
        men_giro('Insercom Informatica Ltda.',21,02,20)
        SETCOLOR('n/w*,w+/n+,,,w+/n')
        DEVPOS(24,33);DEVOUT('Aguarde, Gerando Arquivos..')

        SETCOLOR(vcor[1])
        dbfs()
ENDDO
***********************************************************************
********************************
* CABECALHO DE RECIBOS/RECEITAS
********************************

FUNCTION cabec
**************

*LOCAL mtraco:=REPLI(CHR(205),80)
LOCAL mtraco:=REPLI(CHR(223),80)

IF cab_med = 'VANIA'
        pitalico(.T.)
        @ PROW()+3,00 SAY CHR(14)+CHR(15)
        @ PROW(),01 SAY memp_resa
        @ PROW()+1,00 SAY CHR(18)
        @ PROW(),00 SAY mtraco
        @ PROW()+1,00 SAY CHR(14)
        @ PROW(),00 SAY mesp
        @ PROW()+1,00 SAY CHR(14)+CHR(15)
        @ PROW(),00 SAY crm
        @ PROW()+1,00 SAY CHR(18)
        @ PROW(),00 SAY mtraco
        RETURN NIL
ENDIF
IF cab_med = 'MARLI'
        pitalico(.T.)
        @ PROW()+10,00 SAY ' '
        RETURN NIL
ENDIF
************************** F I M ***********************************
*****************************************
* FUNCAO PARA O RODAPE
*****************************************

FUNCTION rodape
***************

*LOCAL mtraco:=REPLI(CHR(205),80)
LOCAL mtraco:=REPLI(CHR(223),80)

IF cab_med = 'VANIA'
        DEVPOS(PROW()+1,00);DEVOUT(PADC(cpf,80))
        DEVPOS(57,57);DEVOUT(TIME()+' Hrs.')
        pitalico(.F.)
        DEVPOS(58,00);DEVOUT(mtraco)
        pitalico(.T.)
        DEVPOS(59,02);DEVOUT('Rua Ana Camelo da Silva, 163 - Boa Viagem - Fone: (081) 325.0040/465.8558')
        DEVPOS(60,02);DEVOUT('CEP.: 51020 - Recife - Pernambuco.')
        pitalico(.F.)
        RETURN NIL
ENDIF
IF cab_med = 'MARLI'
        RETURN NIL
ENDIF
****************************** f i m **********************************************
***************************
* TELA DE APRESENTACAO
***************************

FUNCTION apresenta
******************

LOCAL tela

tela := SAVESCREEN(00,00,24,79)
SETCOLOR(vcor[1])
IF mmedico = 'VANIA'
        CLEAR
        DEVPOS(01,03);DEVOUT('                  ±±±±±Ü  ±±±±±±Ü ±±±±±±Ü                                 ')
        DEVPOS(02,03);DEVOUT('                  ±±Ûß±±Û ±±Ûß±±Û ±±Ûß±±Û                                 ')
        DEVPOS(03,03);DEVOUT('                  ±±Û ±±Û ±±±±±Ûß ±±±±±±Û                                 ')
        DEVPOS(04,03);DEVOUT('                  ±±Û ±±Û ±±Ûß±±Ü ±±Û ±±Û                                 ')
        DEVPOS(05,03);DEVOUT('                  ±±±±±Ûß ±±Û ±±Û ±±Û ±±Û  ±±Ü                            ')
        DEVPOS(06,03);DEVOUT('                   ßßßßß   ßß  ßß  ßß  ßß   ßß                            ')
        DEVPOS(07,03);DEVOUT('±±Ü ±±Ü ±±±±±±Ü ±±±ÜÜ ±±Ü ±±Ü ±±±±±±Ü     ±±Ü     ±±Ü ±±±±±±Ü ±±Ü ±±±±±±Ü ')
        DEVPOS(08,03);DEVOUT('±±Û ±±Û ±±Ûß±±Û ±±Û±±Û±±Û ±±Û ±±Ûß±±Û     ±±Û     ±±Û ±±Ûßßßß ±±Û ±±Ûß±±Û ')
        DEVPOS(09,03);DEVOUT('±±Û ±±Û ±±±±±±Û ±±Û ß±±±Û ±±Û ±±±±±±Û     ±±Û     ±±Û ±±Û±±±Ü ±±Û ±±±±±±Û ')
        DEVPOS(10,03);DEVOUT(' ±±±±Ûß ±±Û ±±Û ±±Û   ±±Û ±±Û ±±Û ±±Û     ±±Û     ±±Û ±±Û ±±Û ±±Û ±±Û ±±Û ')
        DEVPOS(11,03);DEVOUT('  ±±Ûß  ±±Û ±±Û ±±Û   ±±Û ±±Û ±±Û ±±Û     ±±±±±±Ü ±±Û ±±±±±±Û ±±Û ±±Û ±±Û ')
        DEVPOS(12,03);DEVOUT('   ßß    ßß  ßß  ßß    ßß  ßß  ßß  ßß      ßßßßßß  ßß  ßßßßßß  ßß  ßß  ßß ')
        @ 13,01 SAY REPLI('',78)
        SETCOLOR(vcor[7])

        @ 14,03 SAY ' ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿  '
        @ 15,03 SAY ' ³ ÄÄÄÄÜÜÜÜÜÜÜÜÜÜÄÄÄÄ ³Û '
        @ 16,03 SAY ' ³  ÄÄÄÄÄÄÄÜÜÄÄÄÄÄÄÄ  ³Û '
        @ 17,03 SAY ' ³    ÄÄÄÄÄÜÜÄÄÄÄÄ    ³Û '
        @ 18,03 SAY ' ³   ÄÄÄÄÄÄÜÜÄÄÄÄÄÄÄ  ³Û '
        @ 19,03 SAY ' ³ ÄÄÄÄÜÜÜÜÜÜÜÜÜÜÄÄÄÄ ³Û '
        @ 20,03 SAY ' ÃÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ´Û '
        @ 21,03 SAY ' ³Insercom Informatica³Û '
        @ 22,03 SAY ' ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙÛ '
        @ 23,03 SAY '  ßßßßßßßßßßßßßßßßßßßßßß '
        SETCOLOR(vcor[2])
        CAIXA(mold,14,31,22,73,,,.T.)
        SETCOLOR(vcor[1])
        DEVPOS(15,33);DEVOUT('"S M E D" - Sistema de Controle Medico')
        DEVPOS(16,47);DEVOUT('Versao: '+mversao)
        SETCOLOR(vcor[2])
        @ 17,32 TO 17,72
        @ 18,33 SAY 'Av.Bernardo Vieira de Melo, 4243 - s/24'
        @ 19,33 SAY 'Candeias - Jaboatao dos Guararapes - PE'
        @ 20,33 SAY 'TeleFax.: (081) 468.3400               '
        DEVPOS(21,33);DEVOUT('Desenvolvido por:')
        DEVPOS(21,COL()+1);DEVOUT('Jose Helio Beltrao Jr.')
        DEVPOS(23,33);DEVOUT('Pressione qualquer tecla p/Retornnar')
        SETCOLOR(vcor[1])
        INKEY(0)
        CAIXA(mold,00,00,24,79,,tela,.T.)
        RETURN NIL
ENDIF
IF mmedico = 'MARLI'
        CLEAR
        DEVPOS(01,03);DEVOUT('                  ±±±±±Ü  ±±±±±±Ü ±±±±±±Ü                             ')
        DEVPOS(02,03);DEVOUT('                  ±±Ûß±±Û ±±Ûß±±Û ±±Ûß±±Û                             ')
        DEVPOS(03,03);DEVOUT('                  ±±Û ±±Û ±±±±±Ûß ±±±±±±Û                             ')
