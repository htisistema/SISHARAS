        DEVPOS(05,03);DEVOUT('                  ±±±±±Ûß ±±Û ±±Û ±±Û ±±Û  ±±Ü                        ')
        DEVPOS(06,03);DEVOUT('                   ßßßßß   ßß  ßß  ßß  ßß   ßß                        ')
        DEVPOS(07,03);DEVOUT('±±±ÜÜ±±±Ü ±±±±±±Ü ±±±±±±Ü ±±Ü     ±±Ü ±±±±±±Ü ±±±±±±Ü ±±±±±±Ü ±±±±±±Ü ')
        DEVPOS(08,03);DEVOUT('±±Û±±Û±±Û ±±Ûß±±Û ±±Ûß±±Û ±±Û     ±±Û  ßß±±Ûß ±±Ûßßßß  ß±±Ûßß ±±Ûßßßß ')
        DEVPOS(09,03);DEVOUT('±±Û ßß±±Û ±±±±±±Û ±±±±±Ûß ±±Û     ±±Û   ±±Û   ±±±±±Ü    ±±Û   ±±±±±Ü  ')
        DEVPOS(10,03);DEVOUT('±±Û   ±±Û ±±Û ±±Û ±±Ûß±±Ü ±±Û     ±±Û  ±±Û    ±±Ûßßß    ±±Û   ±±Ûßßß  ')
        DEVPOS(11,03);DEVOUT('±±Û   ±±Û ±±Û ±±Û ±±Û ±±Û ±±±±±±Ü ±±Û ±±±±±±Ü ±±±±±±Ü   ±±Û   ±±±±±±Ü ')
        DEVPOS(12,03);DEVOUT(' ßß    ßß  ßß  ßß  ßß  ßß  ßßßßßß  ßß  ßßßßßß  ßßßßßß    ßß    ßßßßßß ')

        @ 13,01 SAY REPLI('',78)
        SETCOLOR(vcor[7])

        @ 14,03 SAY ' ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿  '
        @ 15,03 SAY ' ³ ÄÄÄÄÜÜÜÜÜÜÜÜÜÜÄÄÄÄ ³Û '
        @ 16,03 SAY ' ³  ÄÄÄÄÄÄÄÜÜÄÄÄÄÄÄÄ  ³Û '
